    "typescript"
  ],
  "author": "gizak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gizak/batch/issues"
  },
  "homepage": "https://github.com/gizak/batch#readme",
  "devDependencies": {
    "@types/async": "^2.0.32",
    "@types/bunyan": "0.0.30",
    "@types/joi": "^9.0.32",
    "@types/koa": "^2.0.33",
    "@types/lodash": "^4.14.37",
    "@types/node": "*",
    "@types/pouchdb": "*",
    "@types/shortid": "0.0.28",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^5.2.2",
    "jest": "*",
    "request": "^2.75.0",
    "request-promise-native": "*",
    "ts-jest": "^0.1.8",
    "typescript": "*"
  },
  "dependencies": {
    "async": "^2.1.1",
    "bunyan": "^1.8.3",
    "dev-null": "^0.1.1",
